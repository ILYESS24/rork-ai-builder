{
  "name": "rork-ai-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --format json --output-file eslint-report.json",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=lcov",
    "test:fuzzing": "jest --testPathPattern=fuzzing.test.ts --verbose",
    "test:unit": "jest --testPathIgnorePatterns=fuzzing.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:fuzzing && npm run test:e2e",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "setup": "npm install && npm run db:generate && npm run db:push",
    "clean": "rm -rf .next node_modules/.cache",
    "prepare": "husky install",
    "turbo": "turbo",
    "vite": "vite",
    "nx": "nx",
    "deploy": "git add . && git commit -m \"Deploy to Render - $(date)\" && git push origin main",
    "deploy:render": "npm run build && npm run deploy",
    "render:build": "npm ci --legacy-peer-deps && npx prisma generate && npm run build",
    "render:start": "npm start",
    "security:scan": "snyk test",
    "security:monitor": "snyk monitor",
    "quality:check": "npm run lint && npm run type-check && npm run test:coverage",
    "ci": "npm run quality:check && npm run test:all"
  },
